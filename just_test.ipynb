    "import yfinance as yf\n",
    "\n",
    "msft = yf.Ticker(\"FVI.TO\")\n",
    "\n",
    "# get stock info\n",
    "msft.info\n",
    "\n",
    "# get historical market data\n",
    "hist = msft.history(period=\"max\")\n",
    "\n",
    "# show actions (dividends, splits)\n",
    "\n",
    "msft.financials\n",
    "hist"
   ]
  },
