   "source": [
    "with open('get_dict_t.txt') as f:\n",
    "    lines = f.readlines()\n",
    "\n",
    "f.close()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 96,
   "metadata": {},
   "outputs": [],
   "source": [
    "stock_index = []\n",
    "for i in lines[2:8]:\n",
    "    temp_i = i.replace(\"\\n\",\"\")\n",
    "    stock_index.append(temp_i[3:])"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 97,
   "metadata": {},
   "outputs": [],
   "source": [
    "stock_index_line_count = []\n",
    "for line,count_line in zip(lines,range(len(lines))):\n",
    "    temp_line = line.replace(\"\\n\",\"\")\n",
    "    if temp_line in stock_index:\n",
    "        stock_index_line_count.append(count_line)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 100,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "S&P500 No\n",
      "HK50 HK\n",
      "NIFTY50 NS\n",
      "TSX TO\n",
      "GB100 L\n",
      "JP225 T\n"
     ]
    }
   ],
   "source": [
    "company_dict = {}\n",
    "for line_count,indices in zip(stock_index_line_count,stock_index):\n",
    "    \n",
    "    index, stock_yahoo=indices.split(\" \")\n",
    "    print(index,stock_yahoo)\n",
    "\n",
    "    for company in range(1,11):\n",
    "        line_values = lines[line_count + company].replace(\"\\n\",\"\")\n",
    "        line_values = line_values.replace(\"\\t\",\"\")\n",
    "        \n",
    "        company_stock = line_values[:line_values.find(\" \")]\n",
    "        company_name =  line_values[line_values.find(\" \")+1:]\n",
    "        \n",
    "        if stock_yahoo != \"No\":\n",
    "            company_dict[str(company_stock)+\".\"+stock_yahoo] = company_name\n",
    "        else:\n",
    "            company_dict[str(company_stock)] = company_name\n"
   ]
  },
